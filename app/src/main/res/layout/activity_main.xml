<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.luciano.red.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="582dp"
        android:background="@drawable/vermelhodegradepp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-4dp">

        <Button
            android:id="@+id/btAuditoria"
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:onClick="OnClickBtAuditoria"
            android:text="@string/btAuditoria_name" />

        <Button
            android:id="@+id/btDados"
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:onClick="OnClickBtDados"
            android:text="@string/btDados_name" />

    </LinearLayout>


</LinearLayout>
